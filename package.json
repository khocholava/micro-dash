{
  "name": "micro-dash-platform",
  "version": "7.3.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dtslint": "dtslint --expectOnly --localTs node_modules/typescript/lib projects/micro-dash/src/typing-tests",
    "calc-sizes": "ng build --prod micro-dash && cd projects/calc-sizes && ts-node -O \"{\\\"module\\\": \\\"commonjs\\\"}\" calc-sizes",
    "docs": "cd docs && typedoc ../projects/micro-dash/src/lib",
    "prettier-all": "prettier --write \"**/*.{html,js,json,md,scss,ts,yml}\"",
    "release": "yarn docs && git add docs/* && standard-version --commit-all && yarn build --prod micro-dash"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.0",
    "@angular/common": "~10.0.0",
    "@angular/compiler": "~10.0.0",
    "@angular/core": "~10.0.0",
    "@angular/forms": "~10.0.0",
    "@angular/platform-browser": "~10.0.0",
    "@angular/platform-browser-dynamic": "~10.0.0",
    "@angular/router": "~10.0.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "utility-types": "~3.10.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular-devkit/build-ng-packagr": "~0.1000.0",
    "@angular/cli": "~10.0.0",
    "@angular/compiler-cli": "~10.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^12.11.1",
    "@types/prettier": "^1.18.2",
    "@types/sinon": "^7.0.11",
    "codelyzer": "^6.0.0-next.1",
    "coveralls": "^3.0.6",
    "dtslint": "^2.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lodash-es": "^4.17.10",
    "ng-packagr": "^10.0.0",
    "prettier": "^1.18.2",
    "protractor": "~7.0.0",
    "s-ng-dev-utils": "^1.0.0",
    "sinon": "^8.1.1",
    "source-map-explorer": "^2.0.1",
    "standard-version": "^7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typedoc": "^0.16.9",
    "typescript": "~3.9.5"
  },
  "prettier": {
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "standard-version": {
    "scripts": {
      "postbump": "ts-node -O \"{\\\"module\\\": \\\"commonjs\\\"}\" standard-version-postbump",
      "postchangelog": "prettier --write CHANGELOG.md",
      "precommit": "git add projects/micro-dash/package.json"
    }
  }
}
